    "style-loader": "^0.13.1",
    "traverse": "^0.6.6",
    "ts-helpers": "^1.1.1",
    "tslint": "^3.10.2",
    "tslint-loader": "^2.1.4",
    "typedoc": "^0.4.4",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "unminified-webpack-plugin": "^1.0.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "0.0.5",
